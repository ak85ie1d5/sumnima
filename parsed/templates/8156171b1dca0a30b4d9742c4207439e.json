{
  "components/common/category-homepage": "<ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n    {{#each categories}}\r\n    <li class=\"navPages-item\">\r\n        {{#if children}}\r\n        <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\r\n            href=\"{{url}}\"\r\n            data-collapsible=\"navPages-{{id}}\"\r\n            aria-label=\"{{name}}\"\r\n            >\r\n            {{#if image}}<img src=\"{{getImage image \"thumbnail\"}}\">{{/if}}<span>{{name}}</span>\r\n            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n        </i>\r\n    </a>\r\n    <div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n        <ul class=\"navPage-subMenu-list\">\r\n            <li class=\"navPage-subMenu-item-parent\">\r\n                <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{#if image}}<img src=\"{{getImage image \"thumbnail\"}}\">{{/if}}<span>{{lang 'category.view_all.name' category=name}}</span></a>\r\n            </li>\r\n            {{#each children}}\r\n            <li class=\"navPage-subMenu-item-child\">\r\n                {{#if children}}\r\n                <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\r\n                    {{#if image}}<img src=\"{{getImage image \"thumbnail\"}}\">{{/if}}<span>{{name}}</span><i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                </a>\r\n                {{> components/common/navigation-dropdown}}\r\n                {{else}}\r\n                <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{#if image}}<img src=\"{{getImage image \"thumbnail\"}}\">{{/if}}<span>{{name}}</span></a>\r\n                {{/if}}\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n    </div>\r\n    {{else}}\r\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\r\n        href=\"{{url}}\"\r\n        aria-label=\"{{name}}\"\r\n        >\r\n        {{#if image}}<img src=\"{{getImage image \"thumbnail\"}}\">{{/if}}<span>{{name}}</span>\r\n    </a>\r\n    {{/if}}\r\n</li>\r\n{{/each}}\r\n</ul>\r\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{#if image}}<img src=\"{{getImage image \"thumbnail\"}}\" alt=\"{{name}}\">{{/if}}<span>{{lang 'category.view_all.name' category=name}}</span></a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\n                        {{#if image}}<img src=\"{{getImage image \"thumbnail\"}}\" alt=\"{{name}}\">{{/if}}<span>{{name}}</span><i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{#if image}}<img src=\"{{getImage image \"thumbnail\"}}\" alt=\"{{name}}\">{{/if}}<span>{{name}}</span></a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n"
}